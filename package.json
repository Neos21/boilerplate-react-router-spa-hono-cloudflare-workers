{
  "name": "boilerplate-react-router-spa-hono-cloudflare-workers",
  "description": "Boilerplate : React Router v7 SPA + Hono + Cloudflare Workers",
  "private": true,
  "type": "module",
  "scripts": {
    "?dev"    : "echo '@react-router/dev 内の React Router CLI を使って Vite による開発サーバを起動する・`isbot` パッケージが勝手にインストールされる'",
     "dev"    : "react-router dev --host",
    "?build"  : "echo 'React Router CLI が Vite を使用して本番ビルドする'",
     "build"  : "react-router build",
    "?preview": "echo 'Vite ビルド後に Wrangler の開発サーバを起動する'",
     "preview": "npm run build && wrangler dev",
    "?deploy" : "echo 'Vite ビルド後に Cloudflare Workers にデプロイする'",
     "deploy" : "npm run build && wrangler deploy"
  },
  "author": "Neo <neos21@gmail.com> (https://neos21.net/)",
  "license": "MIT",
  "homepage": "https://github.com/Neos21/boilerplate-react-router-spa-hono-cloudflare-workers#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neos21/boilerplate-react-router-spa-hono-cloudflare-workers.git"
  },
  "bugs": {
    "url": "https://github.com/Neos21/boilerplate-react-router-spa-hono-cloudflare-workers/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Neos21"
  },
  "dependencies": {
    "@react-router/node": "7.9.1",
    "hono": "4.9.8",
    "isbot": "5.1.30",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router": "7.9.1"
  },
  "devDependencies": {
    "@hono/vite-dev-server": "0.21.0",
    "@react-router/dev": "7.9.1",
    "@types/node": "24.5.2",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "hono-react-router-adapter": "0.6.5",
    "typescript": "5.9.2",
    "vite": "7.1.7",
    "wrangler": "4.38.0"
  }
}
